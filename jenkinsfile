pipeline {
    agent any

    stages {

        stage('Checkout')
        {
            steps
            {
                script
                {
                    cleanWs cleanWhenAborted: false, cleanWhenFailure: false, cleanWhenNotBuilt: false, cleanWhenSuccess: false, cleanWhenUnstable: false
                    git branch: "$params.Branch_Name", credentialsId: "sai366chawan", url: "https://github.com/sai366chawan/test-jenkins.git"
                }
            }
        }
    }
}

